@@ -607,7 +607,7 @@ BEGIN_EVENT_TABLE(AmayaWindow, wxFrame)
   EVT_IDLE(      AmayaWindow::OnIdle ) // Process a wxEVT_IDLE event  
   EVT_ACTIVATE(  AmayaWindow::OnActivate )
