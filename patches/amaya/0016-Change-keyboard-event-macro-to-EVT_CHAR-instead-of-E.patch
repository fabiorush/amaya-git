-  EVT_CHAR_HOOK( AmayaWindow::OnChar )
+  EVT_CHAR( AmayaWindow::OnChar )
 #else /* !_WINDOWS  && ! MACOS */
   //  EVT_CHAR( AmayaWindow::OnChar )
 #endif /* !_WINDOWS  && ! MACOS */
